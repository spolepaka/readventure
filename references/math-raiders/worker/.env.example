# SpacetimeDB connection
SPACETIMEDB_URI=ws://localhost:3000
SPACETIMEDB_MODULE=math-raiders

# SpacetimeDB authentication (required for RLS access to timeback_event_queue)
# Get your token: spacetime login show --token
SPACETIMEDB_TOKEN=your-spacetimedb-owner-token-here

# TimeBack API (use staging for local dev)
TIMEBACK_API_URL=https://caliper-staging.alpha-1edtech.com
TIMEBACK_AUTH_URL=https://alpha-auth-development-idp.auth.us-west-2.amazoncognito.com/oauth2/token

# TimeBack OAuth (get from TimeBack team)
TIMEBACK_CLIENT_ID=your-client-id
TIMEBACK_CLIENT_SECRET=your-client-secret

# Worker settings
WORKER_POLL_INTERVAL_MS=5000
NODE_ENV=development
